<!-- ko with: albums -->
<h2>Artists and Albums</h2>
<h3>Click for detailed list</h3>
<ul id="albumListing" data-bind="foreach: artists">
    <li data-bind="click: $parent.getAlbumList">
        <span data-bind="text: artist_name"></span>
        <ul data-bind="visible: showDetails , foreach: album">
            <li>
                <span data-bind="text: album_title"></span>
            </li>
        </ul>
    </li>
</ul>
<!-- /ko -->
<!-- ko with: movies -->
<h2>Movies</h2>
<ul id="movieListing" data-bind="foreach: movies">
    <li>
        <span data-bind="text: movie_name"></span>
    </li>
</ul>
