<!-- List of possible actions; autogenerated from self.ViewList in app.viewmodel.js -->
<ul class="options" data-bind="foreach: ViewList">
    <li>
        <a data-bind="attr:{href: link}"> <span data-bind="text:name"></span></a>
    </li>
</ul>


